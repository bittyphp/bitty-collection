{
    "name": "bittyphp/bitty-collection",
    "description": "Simple collections.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Kingsley",
            "email": "bizurkur@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bitty\\Collection\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitty\\Tests\\Collection\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
